<h1><%= title %></h1>

<% if (accountData.account_type !== "Client") { %>
  <h2>Welcome <%= accountData.account_firstname %> (<%= accountData.account_type %>)</h2>
<% } %>

<p>You&apos;re logged in.</p>

<p>
  <a href="/account/update/<%= accountData.account_id %>">
    Update Account Information
  </a>
</p>

<% if (accountData.account_type === "Employee" || accountData.account_type === "Admin") { %>
  <h3>Inventory Management</h3>
  <p>
    <a href="/inv/">Go to Inventory Management</a>
  </p>
<% } %>
